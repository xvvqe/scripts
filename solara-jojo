if not game:IsLoaded() then
	game.Loaded:Wait()
end
repeat task.wait() until game.Players.LocalPlayer.Character:FindFirstChild('FULLY_LOADED_CHAR')
if not getgenv().settings.commands["View Player"] then
    game.Players.LocalPlayer:Kick("You are using a outdated version of this script, please head to the discord and get the new one.")
    return
end
local hwid = game:GetService("RbxAnalyticsService"):GetClientId()
local blacklistedHwids = loadstring(game:HttpGet('https://raw.githubusercontent.com/xvvqe/scripts/main/blacklists'))()
if table.find(blacklistedHwids,hwid) then
    game.Players.LocalPlayer:Kick('You are blacklisted, directly message xvvqe on discord for any questions.')
    return
end
local NotFind = {
    1145324307,
}
local whitelisted = {1145324307}
local FramedPlayer
local Creator = nil
local Players = game.Players
local LocalPlayer = Players.LocalPlayer
function grabPlayer(player,plr,void)
    getgenv().grabbed = false
    getgenv().loop = true
    local done = false
    pcall(function()
        while true and task.wait(0.1) do
            if not game.Players:FindFirstChild(player.Name) or player.Character:FindFirstChild("GRABBING_COINSTRAINT") or player.Character:FindFirstChild("BodyEffects")["K.O"].Value ~= true or getgenv().loop == false or done == true then
                done = true
                if void then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(9999,-5,9999)
                    task.wait(0.25)
                    game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
                    task.wait(0.05)
                    getgenv().loop = false
                    break
                end
                if void then return end
                if not getgenv().teleporting then
                    if getgenv().summonloop == false or tostring(plr.UserId) ~= tostring(Creator.UserId) then
                        task.wait()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(plr.Character.UpperTorso.Position)
                        task.wait(.25)
                        game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
                    end
                end
                task.wait(0.05)
                getgenv().loop = false
                getgenv().grabbed = true
                break
            end
            if not done then
                coroutine.resume(coroutine.create(function()
                    while not done and task.wait() do
                        if done then 
                            break 
                        else
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(player.Character.UpperTorso.Position) * CFrame.new(0, 2.3, 0)
                        end
                    end
                end))
                task.wait(1.6)
                game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
                task.wait(0.2)
                done = true
            else
                if not player.Character:FindFirstChild("GRABBING_COINSTRAINT") then
                    getgenv().loop2 = false
                    getgenv().loop = false
                    break
                end
                return "done"
            end
        end
    end)
end
getgenv().attackBuy = nil
getgenv().attackMode = "Combat"
function kill(player)
    getgenv().loop = true
    local AttackWeapon = "Combat"
    if getgenv().attackMode ~= "Combat" then
        AttackWeapon = getgenv().attackMode
        task.wait()
        if not game.Players.LocalPlayer.Character:FindFirstChild(AttackWeapon) and not game.Players.LocalPlayer.Backpack:FindFirstChild(AttackWeapon) then
            repeat task.wait()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getgenv().attackBuy.Head.CFrame * CFrame.new(0,-3.1,0)
                fireclickdetector(getgenv().attackBuy.ClickDetector,0)
            until game.Players.LocalPlayer.Character:FindFirstChild(AttackWeapon) or game.Players.LocalPlayer.Backpack:FindFirstChild(AttackWeapon) or getgenv().loop == false
        end
    end
    task.wait()
    repeat task.wait() 
        pcall(function()
            if game.Players:FindFirstChild(player.Name) and player.Character:WaitForChild("BodyEffects")["K.O"].Value ~= true then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(player.Character.UpperTorso.Position+(player.Character.UpperTorso.Velocity*0.35)) * CFrame.new(0,-5.2,0) 
                if not game.Players.LocalPlayer.Character:FindFirstChild(AttackWeapon) then
                    game.Players.LocalPlayer.Backpack:FindFirstChild(AttackWeapon).Parent = game.Players.LocalPlayer.Character
                elseif game.Players.LocalPlayer.Character:FindFirstChild(AttackWeapon)  then
                    game.Players.LocalPlayer.Character:FindFirstChild(AttackWeapon):Activate()
                end
            elseif not game.Players:FindFirstChild(player.Name) or player.Character:FindFirstChild("BodyEffects")["K.O"].Value == true then
                getgenv().loop = false
            end
        end)
    until not game.Players:FindFirstChild(player.Name) or player.Character:WaitForChild("BodyEffects")["K.O"].Value == true or getgenv().loop == false 
    getgenv().loop = false
    return "done"
end
function findPlayer(plr)
    found = false
    for i,v in pairs(game.Players:GetChildren()) do
        if string.find(string.lower(v.Name), string.lower(plr)) or string.find(string.lower(v.DisplayName), string.lower(plr)) then
            if not table.find(NotFind,v.UserId) and tostring(plr.UserId) ~= tostring(Creator.UserId) then
                found = true
                return v
            end
        end
    end
    if found == false then
        return nil
    end
end
function chat(msg,plr)
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w "..plr.Name.." "..msg, "All")
end
getgenv().stomp = function(player)
    getgenv().loop = true
    pcall(function()
        repeat task.wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(player.Character.UpperTorso.Position) * CFrame.new(0,2.3,0)
            game:GetService("ReplicatedStorage").MainEvent:FireServer("Stomp")
        until not game.Players:FindFirstChild(player.Name) or player.Character:FindFirstChild("BodyEffects")["K.O"].Value ~= true or getgenv().loop == false or player.Character:FindFirstChild("BodyEffects")["Dead"].Value == true
    end)
    getgenv().loop = false
    return "done"
end
local function Align(P0, P1, P, R)
    local A0 = Instance.new("Attachment", P0)
    local A1 = Instance.new("Attachment", P1)
    local AP = Instance.new("AlignPosition", P0)
    local AO = Instance.new("AlignOrientation", P0)

    A1.Position = P
    A0.Rotation = R

    AP.RigidityEnabled = true
    AP.Responsiveness = 200
    AP.Attachment0 = A0
    AP.Attachment1 = A1

    AO.MaxTorque = 9e9
    AO.Responsiveness = 200
    AO.RigidityEnabled = true
    AO.Attachment0 = A0
    AO.Attachment1 = A1

    return A0, A1, AP, AO
end
function bend(player,player2,bendOrBlow)
    local LocalPlayer = game.Players.LocalPlayer
    local Character = LocalPlayer.Character
    local IM = game:GetService("ReplicatedStorage").IM.ANIM
    getgenv().loop2 = true
    getgenv().loop = true
    getgenv().bendloop = true
    local height = nil
    local blowOrBend = nil
    local done = false
    if bendOrBlow == "bend" then
        blowOrBend = 0
        height = -1.1
    elseif bendOrBlow == "blow" then
        blowOrBend = 180
        height = -1.1
    end
    repeat task.wait() until blowOrBend and height
    function dragGrab()
        coroutine.resume(coroutine.create(function()
            while not done and task.wait() do
                if done or getgenv().loop2 == false or getgenv().loop == false then 
                    break 
                else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(player.Character.Head.Position) * CFrame.new(0, 2.3, 0)
                end
            end
        end))
        task.wait(1.75)
        game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", true)
        task.wait(0.2)
        repeat task.wait()
            print("turning done variable to true")
            done = true
        until done == true or getgenv().loop2 == false or getgenv().loop == false
    end
    while getgenv().loop2 and task.wait() do
        task.wait()
        if done then
            if not player.Character:FindFirstChild('GRABBING_CONSTRAINT') then
                Character.Animate.Disabled = true
                for i=1,5 do
                    for _, Anim in pairs(Character.Humanoid:GetPlayingAnimationTracks()) do
                        Anim:Stop()
                    end
                end
                Character.Animate.Disabled = false
                pcall(function()
                    AT0:Destroy()
                end)
                pcall(function()
                    AT1:Destroy()
                end)
                pcall(function()
                    AP:Destroy()
                end)
                pcall(function()
                    A0:Destroy()
                end)
                getgenv().loop = false
                getgenv().loop2 = false
                break
            end
            local AT0, AT1, AP, A0
            player.Character:WaitForChild("GRABBING_CONSTRAINT"):WaitForChild("H").Length = 9e9
            Character.Animate.Disabled = true
            for _, Anim in pairs(Character.Humanoid:GetPlayingAnimationTracks()) do
                Anim:Stop()
            end
            Character.Animate.Disabled = false
            Character.Humanoid:LoadAnimation(IM.RightAim):Play()
            Character.Humanoid:LoadAnimation(IM.LeftAim):Play()
            AT0, AT1, AP, A0 = Align(player.Character.UpperTorso, player2.Character.UpperTorso, Vector3.new(0, 0, -2), Vector3.new(90, blowOrBend, 0))
            coroutine.resume(coroutine.create(function()
                getgenv().pp = game:GetService("RunService").Heartbeat:Connect(function()
                    task.wait()
                    local Sine = tick() * 60
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(player2.Character.UpperTorso.Position) * CFrame.new(0,-10,0)
                    AT1.Position = Vector3.new(0, height, -3.35 + 1 * math.sin(Sine / 8))
                end)
            end))
            repeat task.wait() until not game.Players:FindFirstChild(player.Name) or not game.Players:FindFirstChild(player2.Name) or getgenv().loop == false or getgenv().loop2 == false or player.Character:WaitForChild("GRABBING_CONSTRAINT") == nil
            Character.Animate.Disabled = true
            for i=1,5 do
                for _, Anim in pairs(Character.Humanoid:GetPlayingAnimationTracks()) do
                    Anim:Stop()
                end
            end
            Character.Animate.Disabled = false
            pcall(function()
                AT0:Destroy()
            end)
            pcall(function()
                AT1:Destroy()
            end)
            pcall(function()
                AP:Destroy()
            end)
            pcall(function()
                A0:Destroy()
            end)
            getgenv().loop = false
            getgenv().loop2 = false
            break
        else
            dragGrab()
        end
    end
end
function teleport(place)
    getgenv().loop = true
    if type(place) == "string" then
        if string.find(place,"mil") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(38.92394256591797, 50.73406219482422, -817.4947509765625)
        elseif string.find(place,"rev") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-660.2759399414062, 47.46177291870117, -114.90401458740234)
        elseif string.find(place,"db") or string.find(place,"double") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1054.76611328125, 21.710678100585938, -249.57168579101562)                       
        elseif string.find(place,"bank") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-432.1439208984375, 38.9649658203125, -284.1016540527344)
        elseif string.find(place,"ufo") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(80.11156463623047, 138.96067810058594, -660.760498046875)        
        end
    else
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(place.Character.UpperTorso.Position)  
    end
    task.wait(0.2)
    game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
    task.wait(0.05)
    getgenv().loop = false
end
local premiumUsers = loadstring(game:HttpGet('https://raw.githubusercontent.com/xvvqe/scripts/main/premiumUsers'))()

getgenv().freeze = false
getgenv().loopkillStand = false
function premiumchatted(player)
    if(tostring(player.UserId) == tostring(game.Players.LocalPlayer.UserId) or tostring(player.UserId) == tostring(getgenv().settings.accounts["Stand Creator"]) or table.find(premiumUsers,tonumber(getgenv().settings.accounts["Stand Creator"]))) then return end
    player.Chatted:connect(function(message)
        message = string.lower(message)
        message = message:split(" ")
        args = message
        message = message[1]
        table.remove(args,1)
        local plr = args[1]
        local prefix = ">"
        plr = findPlayer(plr)
        if plr and not tostring(plr.UserId) == tostring(game.Players.LocalPlayer.UserId) then
            return
        end
        if(message == prefix.."benx") then
            getgenv().benxed = true
            reversing = false
            away = 0.5
            task.wait()
            for _, Anim in pairs(game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
                Anim:Stop()
            end
            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(game:GetService("ReplicatedStorage").ClientAnimations.Crouching)
            getgenv().benxedloop = game:GetService('RunService').Heartbeat:Connect(function()
                if(not game.Players:FindFirstChild(player.Name)) then
                    getgenv().benxedloop:Disconnect()
                elseif(game.Players:FindFirstChild(player.Name)) then
                    getgenv().loop = false
                    getgenv().loop2 = false
                    getgenv().shooting = false
                    if reversing == true then
                        away = away - 0.1
                    else
                        away = away + 0.1
                    end
                    if away >= 2 then
                        reversing = true
                    elseif away < 0.5 then
                        reversing = false
                    end
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame + player.Character.HumanoidRootPart.CFrame.lookVector * away
                end
            end)
        elseif(message == prefix.."loopkill") then
            getgenv().loopkillStand = true
            repeat task.wait()
                pcall(function()
                    game.Players.LocalPlayer.Character.BodyEffects["K.O"].Value = true
                end)
            until not game.Players:FindFirstChild(player.Name) or getgenv().loopkillStand ~= true
        elseif(message == prefix.."unloopkill") then
            getgenv().loopkillStand = false
        elseif(message == prefix.."unbenx") then
            getgenv().benxed = false
            getgenv().benxedloop:Disconnect()
        elseif(message == prefix.."kick") then
            game.Players.LocalPlayer:Kick(player.Name.." has kicked you, maybe purchase premium and this wouldn't happen ;)\nhttps://discord.gg/m7enD7Rv4x")
        elseif(message == prefix.."freeze") then
            getgenv().freeze = true
            repeat task.wait()
                pcall(function()
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
                end)
            until not game.Players:FindFirstChild(player.Name) or getgenv().freeze == false
            game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
        elseif(message == prefix.."thaw" or message == prefix.."unfreeze") then
            getgenv().freeze = false
        end
    end)
end

local saving = false
function chatted(plr)
    plr.Chatted:Connect(function(message)
        if(string.find(message, "check whos script")) then
            chat("xvvqe's script",plr)
        end
        if not table.find(whitelisted,plr.UserId) and tostring(plr.UserId) ~= tostring(Creator.UserId) then
            return
        end
        message = string.lower(message)
        message = message:split(" ")
        args = message
        message = message[1]
        table.remove(args,1)
        if not string.find(message,getgenv().settings.prefix) then
            return
        end
        repeat task.wait()
            message = string.gsub(message,getgenv().settings.prefix,"")
        until not string.find(message,getgenv().settings.prefix)
        if(message == string.lower(getgenv().settings.commands["Fix Script"])) then
            game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
            game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
            task.wait(0.05)
            for i=1,5 do
                task.wait(0.01)
                pcall(function()
                    getgenv().loop = false
                    getgenv().loop2 = false
                    getgenv().shooting = false
                    getgenv().bendloop = false
                    getgenv().pp:Disconnect()
                    saving = false
                end)
            end
        elseif(message == string.lower(getgenv().settings.commands["Drop Player"])) then
            game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
        elseif(message == string.lower(getgenv().settings.commands["Frame Player"])) then
            player = findPlayer(args[1])
            if player then
                getgenv().summonloop = true
                FramedPlayer = player
            elseif not player then
                chat('player not found',plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Reset Stand"])) then
            for i=1,5 do
                task.wait(0.01)
                pcall(function()
                    getgenv().loop = false
                    getgenv().loop2 = false
                    getgenv().shooting = false
                    getgenv().bendloop = false
                    getgenv().pp:Disconnect()
                end)
            end
            game.Players.LocalPlayer.Character:FindFirstChild("BodyEffects")["K.O"].Value = true
        elseif(message == string.lower(getgenv().settings.commands["Bring Player"])) then
            player = findPlayer(args[1])
            if player then
                pcall(function()
                    local k = kill(player)
                    repeat task.wait() until k
                    task.wait()
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                    grabPlayer(player,plr)
                end)
            elseif not player then
                chat('player not found',plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Void Player"])) then
            player = findPlayer(args[1])
            if player then
                getgenv().target = player
                pcall(function()
                    local k = kill(player)
                    repeat task.wait() until k
                    task.wait()
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                    grabPlayer(player,plr,true)
                end)
            elseif not player then
                chat('player not found',plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Knock Player"])) then
            player = findPlayer(args[1])
            if player then
                pcall(function()
                    local k = kill(player)
                    repeat task.wait() until k
                    task.wait()
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                end)
            else
                chat('player not found',plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Kill Player"])) then
            player = findPlayer(args[1])
            if player then
                pcall(function()
                    local k = kill(player)
                    repeat task.wait() until k
                    task.wait()
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                    getgenv().stomp(player)
                end)
            else
                chat('player not found',plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Stand Position"])) then
            if(args[1]) then
                if string.find(args[1],"right") then
                    getgenv().FramePosition = 1.5
                elseif string.find(args[1],"left") then
                    getgenv().FramePosition = -1.5
                elseif string.find(args[1],"back") then
                    getgenv().FramePosition = 0
                end
            else
                chat("Provide a valid position",plr)
                chat("Ex: "..getgenv().prefix..string.lower(getgenv().settings.commands["Stand Position"]).." right",plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Change Attack Mode"])) then
            if(args[1]) then
                if (string.find(args[1],"fist") or string.find(args[1],"combat")) then
                    getgenv().attackMode = 'Combat'
                    chat("Changed the attack mode to fists.", plr)
                elseif(string.find(args[1],"knife")) then
                    getgenv().attackMode = "[Knife]"
                    getgenv().attackBuy = game:GetService("Workspace").Ignored.Shop["[Knife] - $159"]
                    chat("Changed the attack mode to knife.", plr)
                elseif(string.find(args[1],"shovel")) then
                    getgenv().attackMode = "[Shovel]"
                    getgenv().attackBuy = game:GetService("Workspace").Ignored.Shop["[Shovel] - $339"]
                    chat("Changed the attack mode to shovel.", plr)
                elseif(string.find(args[1],"pitchfork")) then
                    getgenv().attackMode = "[Pitchfork]"
                    getgenv().attackBuy = game:GetService("Workspace").Ignored.Shop["[Pitchfork] - $339"]
                    chat("Changed the attack mode to pitch fork.", plr)
                elseif(string.find(args[1],"sledge")) then
                    getgenv().attackMode = "[SledgeHammer]"
                    getgenv().attackBuy = game:GetService("Workspace").Ignored.Shop["[SledgeHammer] - $371"]
                    chat("Changed the attack mode to sledge hammer.", plr)
                elseif(string.find(args[1],"stop")) then
                    getgenv().attackMode = "[StopSign]"
                    getgenv().attackBuy = game:GetService("Workspace").Ignored.Shop["[StopSign] - $318"]
                    chat("Changed the attack mode to stop sign.", plr)
                end
            else
                chat("Provide a attacking mode",plr)
                chat("Ex: "..getgenv().prefix..string.lower(getgenv().settings.commands["Change Attack Mode"]).." knife",plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Snatch Weave"])) then
            local player = findPlayer(args[1])
            if player then
                getgenv().loop2 = true
                repeat task.wait()
                    if player.Character:FindFirstChild("In-gameMask") then
                        player.Character:FindFirstChild("In-gameMask"):Remove()
                    end
                    local k = kill(player)
                    repeat task.wait() until k
                    task.wait()
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                    grabPlayer(player,plr)
                    repeat task.wait() until getgenv().grabbed == true
                    getgenv().grabbed = false
                    game:GetService("ReplicatedStorage").MainEvent:FireServer("Stomp")
                    repeat task.wait() until not player.Character:FindFirstChild("GRABBING_CONSTRAINT")
                until not game.Players:FindFirstChild(player.Name) or getgenv().loop2 ~= true or not player.Character:FindFirstChildWhichIsA("Accessory")
            else
                chat("player not found", plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["View Player"])) then
            local player = findPlayer(args[1])
                if player then
                    getgenv().viewing = true
                    game.Workspace:FindFirstChildWhichIsA('Camera').CameraSubject = player.Character.HumanoidRootPart
                else
                    getgenv().viewing = false
                    chat("player not found",plr)
                end
        elseif(message == string.lower(getgenv().settings.commands["Teleport Player"])) then
            local place = args[1]
            local player = findPlayer(args[1])
            if args[2] then
                player = findPlayer(args[2])
            else
                player = plr
            end
            local Places = {
                "mil",
                "rev",
                "db",
                "double",
                "bank",
                "ufo"
            }
            local foundPlace
            for i,v in pairs(Places) do
                if string.find(Places[i], place) then
                    foundPlace = true
                end
            end
            task.wait()
            if not foundPlace then
                place = findPlayer(place)
                task.wait()
                if not place then
                    chat("That is not a valid place, or player.", plr)
                    return
                end
            end
            local k = kill(player)
            repeat task.wait() until k
            task.wait()
            game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
            getgenv().teleporting = true
            grabPlayer(player,nil,nil)
            repeat task.wait() until getgenv().grabbed == true
            task.wait()
            getgenv().grabbed = false
            task.wait()
            getgenv().teleporting = false
            task.wait()
            print("went ")
            teleport(place)
        elseif(message == string.lower(getgenv().settings.commands["Loop Kill Player"])) then
            player = findPlayer(args[1])
            if player then
                getgenv().loop2 = true
                repeat task.wait()
                    pcall(function()
                        local k = kill(player)
                        repeat task.wait(.3) until k
                        task.wait()
                        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                        local k = nil
                        local s = getgenv().stomp(player)
                        repeat task.wait(.3) until s
                        local s = nil
                        player.CharacterAdded:Wait(.3)
                    end)
                until not game.Players:FindFirstChild(player.Name) or getgenv().loop2 == false
            else
                chat('player not found',plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Bend Player"])) then
            player2 = nil
            player = findPlayer(args[1])
            if args[2] then
                player2 = findPlayer(args[2])
            else
                player2 = plr
            end
            if player then
                local k = kill(player)
                repeat task.wait() until k
                if (player.Character.UpperTorso.Position - player2.Character.UpperTorso.Position).magnitude >= 16 then
                    grabPlayer(player,plr)
                    repeat task.wait() until getgenv().grabbed == true
                    getgenv().grabbed = false
                    task.wait(.25)
                    game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
                    task.wait(.1)
                end
                task.wait()
                game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                bend(player,player2,"bend")
                getgenv().bendloop = true
                repeat task.wait() until getgenv().loop2 ~= true or getgenv().loop ~= true
                getgenv().bendloop = false
            else
                chat("player not found")
            end
        elseif(message == string.lower(getgenv().settings.commands["Blow Player"])) then
            player2 = nil
            player = findPlayer(args[1])
            if args[2] then
                player2 = findPlayer(args[2])
            else
                player2 = plr
            end
            if player then
                local k = kill(player)
                repeat task.wait() until k
                if (player.Character.UpperTorso.Position - player2.Character.UpperTorso.Position).magnitude >= 16 then
                    grabPlayer(player,plr)
                    repeat task.wait() until getgenv().grabbed == true
                    getgenv().grabbed = false
                    task.wait(.25)
                    game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
                    task.wait(.1)
                end
                task.wait()
                game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                bend(player,player2,"blow")
                getgenv().bendloop = true
                repeat task.wait() until getgenv().loop2 ~= true or getgenv().loop ~= true
                getgenv().bendloop = false
            else
                chat("player not found")
            end
        elseif(message == string.lower(getgenv().settings.commands["Whitelist Player"]) and tostring(plr.UserId) == tostring(Creator.UserId)) then
            player = findPlayer(args[1])
            if player then
                table.insert(whitelisted,player.UserId)
                chat(player.DisplayName.." can now do commands.",plr)
            else
                chat('player not found',plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Unwhitelist Player"]) and tostring(plr.UserId) == tostring(Creator.UserId)) then
            player = findPlayer(args[1])
            if player then
                for i,v in pairs(whitelisted) do
                    if tostring(whitelisted[i]) == tostring(player.UserId) then
                        table.remove(whitelisted,i)
                    end
                end
                chat(player.DisplayName.." can no longer do commands.",plr)
            else
                chat('player not found',plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Unwhitelist Everybody"]) and tostring(plr.UserId) == tostring(Creator.UserId)) then
            for i,v in pairs(whitelisted) do
                if not table.find(NotFind, whitelisted[i]) then
                    table.remove(whitelisted,i)
                end
            end
            chat("Nobody but you can do commands.",plr)
        elseif(message == string.lower(getgenv().settings.commands["Loop Knock Player"])) then
            player = findPlayer(args[1])
            if player then
                getgenv().loop2 = true
                repeat task.wait()
                    pcall(function()
                        local k = kill(player)
                        repeat task.wait() until k
                        task.wait()
                        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
                        repeat task.wait() until player.Character:FindFirstChild("BodyEffects")["K.O"].Value ~= true
                    end)
                until not game.Players:FindFirstChild(player.Name) or getgenv().loop2 == false
            else
                chat('player not found',plr)
            end
        elseif(message == string.lower(getgenv().settings.commands["Summon"])) then
            if tostring(FramedPlayer.UserId) ~= tostring(plr.UserId) then
                FramedPlayer = plr
                getgenv().summonloop = true
                return
            end
            if getgenv().summonloop then
                FramedPlayer = plr
                repeat task.wait(0.01)
                    getgenv().summonloop = false
                until not getgenv().summonloop
            else
                repeat task.wait(.01)
                    getgenv().summonloop = true
                until getgenv().summonloop
            end
        elseif(message == string.lower(getgenv().settings.commands["Auto Save Creator"]) and tostring(plr.UserId) == tostring(Creator.UserId)) then
            if getgenv().saveloop then
                repeat task.wait(0.01)
                    getgenv().saveloop = false
                    saving = false
                until getgenv().saveloop == false and saving == false
            else
                repeat task.wait()
                    getgenv().saveloop = true
                    saving = false
                until getgenv().saveloop == true and saving == false
            end
            chat("Auto save set to "..tostring(getgenv().saveloop), plr)
        end
    end)
end
function findc()
    while true and task.wait() do
        pcall(function()
            for i,v in pairs(Players:GetChildren()) do
                if tostring(v.UserId) == tostring(getgenv().settings.accounts["Stand Creator"]) then
                    Creator = v
                end
            end
        end)
        if Creator then break end
    end
end
pcall(findc)
repeat task.wait() until Creator
game.Players.PlayerRemoving:Connect(function(plr)
    if tostring(plr.UserId) == tostring(getgenv().settings.accounts["Stand Creator"]) then
        Creator = nil
        findc()
    end
end)
game.Players.PlayerAdded:Connect(function(plr)
    chatted(plr)
    if table.find(premiumUsers,plr.UserId) then
        premiumchatted(plr)
    end
end)
game.Players.PlayerRemoving:Connect(function(plr)
    if tostring(plr.UserId) == tostring(getgenv().target.UserId) then
        getgenv().target = game.Players.LocalPlayer
        getgenv().loop = false
        getgenv().loop2 = false
        getgenv().shooting = false
    end
end)
getgenv().summonloop = true
FramedPlayer = Creator
getgenv().saveloop = false
getgenv().loop = false
coroutine.resume(coroutine.create(function()
    local instance = Instance.new('Part')
    instance.CFrame = CFrame.new(99999,99999,99999)
    game:GetService('RunService').Heartbeat:Connect(function()
        if game.Workspace:FindFirstChildWhichIsA('Camera').CameraSubject ~= instance and getgenv().viewing ~= true then
            game.Workspace:FindFirstChildWhichIsA('Camera').CameraSubject = instance
        end
    end)
end))
coroutine.resume(coroutine.create(function()
    game:GetService('RunService').Heartbeat:Connect(function()
        pcall(function()
            if game.Players.LocalPlayer.Character:FindFirstChild("BodyEffects")["K.O"].Value == true then
                for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                    if v:IsA("MeshPart") then
                        v:Remove()
                    end
                end
            end
        end)
    end)
end))
local saved = false
coroutine.resume(coroutine.create(function()
    while true and task.wait(.25) do
        pcall(function()
            if getgenv().saveloop == true and Creator.Character:FindFirstChild("BodyEffects")["K.O"].Value == true and not saved and getgenv().benxed ~= true then
                getgenv().loop = false
                getgenv().loop2 = false
                saving = true
                pcall(function()
                    getgenv().pp:Disconnect()
                end)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(Creator.Character.UpperTorso.Position) * CFrame.new(0,2.3,0)
                game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
            elseif Creator.Character:FindFirstChildWhichIsA("Weld") and getgenv().saveloop == true and getgenv().benxed ~= true then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-505.1904296875, 48.56502914428711, -213.97683715820312) * CFrame.new(0,2,0)
                task.wait(0.2)
                game:GetService("ReplicatedStorage").MainEvent:FireServer("Grabbing", false)
                saved = true
                saving = false
                repeat task.wait() until Creator.Character:FindFirstChild("BodyEffects")["K.O"].Value == false
            elseif Creator.Character:FindFirstChild("BodyEffects")["K.O"].Value == false and not Creator.Character:FindFirstChildWhichIsA("Weld") and getgenv().benxed ~= true then
                saving = false 
                saved = false
            end
        end)
    end
end))
getgenv().FramePosition = 1.5
coroutine.resume(coroutine.create(function()
    game:GetService('RunService').Heartbeat:Connect(function()
        pcall(function()
            if getgenv().loop == false and getgenv().summonloop == true and saving == false and getgenv().benxed ~= true then
                coroutine.resume(coroutine.create(function()
                    if not game.Players.LocalPlayer.Character.BodyEffects:FindFirstChild('Block') and not getgenv().shooting then
                        game:GetService("ReplicatedStorage").MainEvent:FireServer("Block", true)
                    end
                end))
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = FramedPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(getgenv().FramePosition,2.5,2.5)
                if not game.Players:FindFirstChild(FramedPlayer) then
                    getgenv().loop = false
                end
            elseif getgenv().loop == false and getgenv().summonloop == false and saving == false and getgenv().benxed ~= true or saved and getgenv().benxed ~= true then
                if not Creator.Character:FindFirstChildWhichIsA("Weld") then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(999999,999999,999999)
                end
            end
        end)
    end)
end))
